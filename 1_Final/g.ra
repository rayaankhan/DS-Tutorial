LOAD STUDENTS
LOAD ADVISOR
STUD_ADV <- CROSS STUDENTS ADVISOR
STUD_ADV_AGE <- SELECT adv_age > 40 FROM STUD_ADV
STUD_ADV_AGE_EQ <- SELECT adv_id == advisor_id FROM STUD_ADV_AGE
7_FINAL <- PROJECT stud_id, adv_id FROM STUD_ADV_AGE_EQ
PRINT 7_FINAL
CLEAR STUD_ADV
CLEAR STUD_ADV_AGE
CLEAR STUD_ADV_AGE_EQ
CLEAR 7_FINAL