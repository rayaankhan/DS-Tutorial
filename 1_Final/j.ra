LOAD EMPLOYEE
RES <- CROSS EMPLOYEE EMPLOYEE
RES2 <- SELECT EMPLOYEE1_Super_ssn == EMPLOYEE2_Ssn FROM RES
RES3 <- SELECT EMPLOYEE1_Bdate == EMPLOYEE2_Bdate FROM RES2 
10_FINAL <- PROJECT EMPLOYEE1_Ssn, EMPLOYEE1_Super_ssn, EMPLOYEE1_Bdate FROM RES3
RENAME EMPLOYEE1_Ssn TO Ssn FROM 10_FINAL
RENAME EMPLOYEE1_Super_ssn TO Super_ssn FROM 10_FINAL
RENAME EMPLOYEE1_Bdate TO Bdate FROM 10_FINAL
PRINT 10_FINAL
CLEAR RES
CLEAR RES2
CLEAR RES3
CLEAR 10_FINAL